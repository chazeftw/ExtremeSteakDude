        <TextBlock x:Name="Level1_Copy3" Text="Level 2" FontSize="40" HorizontalAlignment="Left" Margin="878,138,0,0" VerticalAlignment="Top"/>
        <TextBlock x:Name="TimeAndName"  Text="{Binding lv1}" FontSize="35" HorizontalAlignment="Left" Margin="118,206,0,0" VerticalAlignment="Top"/>
        <Label x:Name="TimeAndName_Copy" Content="TimeAndNameHere" FontSize="35" HorizontalAlignment="Left" Margin="784,201,0,0" VerticalAlignment="Top"/>
        <Label x:Name="TimeAndName_Copy1" Content="TimeAndNameHere" FontSize="35" HorizontalAlignment="Left" Margin="118,392,0,0" VerticalAlignment="Top"/>
        <Label x:Name="TimeAndName_Copy2" Content="TimeAndNameHere" FontSize="35" HorizontalAlignment="Left" Margin="468,499,0,0" VerticalAlignment="Top"/>
        <Label x:Name="TimeAndName_Copy3" Content="TimeAndNameHere" FontSize="35" HorizontalAlignment="Left" Margin="784,390,0,0" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
