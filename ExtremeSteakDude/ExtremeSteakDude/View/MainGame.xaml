<UserControl x:Class="ExtremeSteakDude.View.MainGame"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExtremeSteakDude.View"
             xmlns:ViewModel="clr-namespace:ExtremeSteakDude.ViewModel"
             mc:Ignorable="d" 
             d:DesignHeight="720" d:DesignWidth="1280"
             PreviewKeyDown="Window_PreviewKeyDown"
             PreviewKeyUp="Window_PreviewKeyUp">
    <Canvas>
        <Image x:Name="player" HorizontalAlignment="Left" Height="32" Margin="0,0,0,0" VerticalAlignment="Top" Width="32" Source="/ExtremeSteakDude;component/Levels/meatboy.jpg" Canvas.Left="{Binding x}" Canvas.Top="{Binding y}"/>
    </Canvas>
</UserControl>
